  "version": "0.0.0",
  "description": "JSX ui kit built for Tooolsâ„¢",
  "type": "module",
  "scripts": {
    "start": "storybook dev -p 6006 --no-open",
    "build": "vite build",
    "build:watch": "vite build --mode dev",
    "build:docs": "storybook build -o docs"
  },
  "author": "Arnaud Juracek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:tooooools/ui.git"
